{
  "name": "nexeval_shared",
  "version": "1.0.0",
  "description": "A shared package which provides some core functionalities, set of rules and standard way to work on Nexeval.",
  "type": "module",
  "private": "false",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "clean": "rimraf dist",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "start": "node dist/index.js",
    "prepare": "pnpm run build"
  },
  "keywords": [],
  "author": "theaiemperor/Arman",
  "license": "AGPL-3.0-or-later",
  "packageManager": "pnpm@10.12.4",
  "dependencies": {
    "axios": "^1.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.17.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.2.1",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.js",
      "require": "./dist/utils/*.cjs.js"
    }
  }
}
